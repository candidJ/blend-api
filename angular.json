{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "projects": {
    "BlendApiApp": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "inlineTemplate": false,
          "inlineStyle": false,
          "style": "scss",
          "skipTests": true
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:module": {},
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "apps/BlendApiApp",
      "sourceRoot": "apps/BlendApiApp/src",
      "prefix": "ba",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "customWebpackConfig": {
              "path": "./extra-webpack.config.js",
              "mergeStrategies": {
                "externals": "replace"
              }
            },
            "outputPath": "dist/apps/BlendApiApp",
            "index": "apps/BlendApiApp/src/index.html",
            "main": "apps/BlendApiApp/src/main.ts",
            "polyfills": "apps/BlendApiApp/src/polyfills.ts",
            "tsConfig": "apps/BlendApiApp/tsconfig.app.json",
            "assets": [
              "apps/BlendApiApp/src/favicon.ico",
              "apps/BlendApiApp/src/assets",
              {
                "glob": "_redirects",
                "input": "apps/BlendApiApp/src",
                "output": "/"
              }
            ],
            "styles": [
              "apps/BlendApiApp/src/styles.scss"
            ],
            "scripts": [],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": true,
            "sourceMap": true,
            "optimization": true,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "assets": [
                "apps/BlendApiApp/src/favicon.ico",
                "apps/BlendApiApp/src/assets"
              ],
              "fileReplacements": [
                {
                  "replace": "apps/BlendApiApp/src/environments/environment.ts",
                  "with": "apps/BlendApiApp/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "serviceWorker": false,
              "commonChunk": true,
              "deleteOutputPath": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "BlendApiApp:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "BlendApiApp:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "BlendApiApp:build"
          }
        }
      }
    },
    "calculator": {
      "projectType": "library",
      "root": "libs/calculator",
      "sourceRoot": "libs/calculator/src",
      "prefix": "blend-api",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "{workspaceRoot}/coverage/libs/calculator"
          ],
          "options": {
            "jestConfig": "libs/calculator/jest.config.ts",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/calculator/src/**/*.ts",
              "libs/calculator/src/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "hacker-news": {
      "projectType": "library",
      "root": "libs/hacker-news",
      "sourceRoot": "libs/hacker-news/src",
      "prefix": "blend-api",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "{workspaceRoot}/coverage/libs/hacker-news"
          ],
          "options": {
            "jestConfig": "libs/hacker-news/jest.config.ts",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/hacker-news/src/**/*.ts",
              "libs/hacker-news/src/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "quotes": {
      "projectType": "library",
      "root": "libs/quotes",
      "sourceRoot": "libs/quotes/src",
      "prefix": "blend-api",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "{workspaceRoot}/coverage/libs/quotes"
          ],
          "options": {
            "jestConfig": "libs/quotes/jest.config.ts",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/quotes/src/**/*.ts",
              "libs/quotes/src/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "shared": {
      "projectType": "library",
      "root": "libs/shared",
      "sourceRoot": "libs/shared/src",
      "prefix": "blend-api",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "{workspaceRoot}/coverage/libs/shared"
          ],
          "options": {
            "jestConfig": "libs/shared/jest.config.ts",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/src/**/*.ts",
              "libs/shared/src/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "weather": {
      "projectType": "library",
      "root": "libs/weather",
      "sourceRoot": "libs/weather/src",
      "prefix": "blend-api",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "{workspaceRoot}/coverage/libs/weather"
          ],
          "options": {
            "jestConfig": "libs/weather/jest.config.ts",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/weather/src/**/*.ts",
              "libs/weather/src/**/*.html"
            ]
          }
        }
      },
      "tags": []
    }
  }
}
